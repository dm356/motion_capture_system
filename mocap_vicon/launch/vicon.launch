<launch>
  <node pkg="mocap_vicon"
    type="mocap_vicon_node"
    name="vicon"
    output="screen">
    <param name="server_address" value="192.168.10.1"/>
    <param name="frame_rate" value="100"/>
    <param name="max_accel" value="10.0"/>
    <param name="publish_tf" value="false"/>
    <param name="fixed_frame_id" value="mocap"/>
    <param name="batch_timeout" value="1"/>
    <!--<rosparam param="model_list">[danaus08]</rosparam>-->
    <rosparam param="batch_list">[danaus]</rosparam>
    <remap from="/vicon/miniquad1/odom" to="/miniquad1/odom"/>
  </node>
</launch>
